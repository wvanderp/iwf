{
  "name": "iwf",
  "version": "0.1.1",
  "description": "improved wikidata format",
  "author": "wvdp",
  "license": "MIT",
  "keywords": [
    "wikidata",
    "wikipedia"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wvanderp/iwf.git"
  },
  "homepage": "https://wvanderp.github.io/iwf/",
  "main": "./lib/index.js",
  "types": "./lib",
  "engines": {
    "node": ">= 16.0.0"
  },
  "browserslist": [
    ">0.3%, last 2 versions, not dead, not op_mini all"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/deep-diff": "^1.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.16",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-woke": "^1.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "source-map-support": "^0.5.21",
    "ts-node": "10.9.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@wmde/wikibase-datamodel-types": "git+https://github.com/wvanderp/WikibaseDataModelTypes.git",
    "axios": "1.5.0",
    "diff-arrays-of-objects": "git+https://github.com/wvanderp/diff-arrays-of-objects.git",
    "qs": "^6.11.2",
    "uuid": "^9.0.0",
    "wikidata-properties": "git+https://github.com/wvanderp/wikidata-properties.git"
  },
  "scripts": {
    "build": "rimraf lib && tsc -P ./tsconfig.build.json",
    "docs": "typedoc --out docs src/index.ts",
    "lint": "npm-run-all -c -s tsc eslint",
    "tsc": "tsc --noEmit",
    "eslint": "eslint --ext ts,js src/ tests/",
    "prepublish": "npm run build",
    "test": "mocha --recursive tests/**/*.spec.ts tests/**/**/*.spec.ts",
    "test:integration": "mocha integration/**/*.spec.ts",
    "coverage:test": "nyc --silent npm run test",
    "coverage:integration": "nyc --silent --no-clean npm run test:integration",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage": "npm run coverage:test && npm run coverage:integration && npm run coverage:report",
    "clean": "rimraf lib/ .nyc_output/ coverage/ docs/"
  }
}
